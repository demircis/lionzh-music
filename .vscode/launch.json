  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Deploy commands",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/deploy-commands.ts",
      "outFiles": ["${workspaceFolder}/**/*.ts"],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run bot",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/index.ts",
      "outFiles": ["${workspaceFolder}/**/*.ts"],
      "console": "integratedTerminal"
    }
  ]
}
